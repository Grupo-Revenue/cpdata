{
  "rewrites": [
    {
      "source": "/((?!api|_next|_static|_vercel|[\\w-]+\\.\\w+).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}